{
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/pdf/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/pdf/(.*)",
      "dest": "/pdf/$1",
      "headers": {
        "Content-Type": "application/pdf"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
